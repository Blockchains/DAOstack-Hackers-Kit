{
  "name": "daico",
  "version": "1.0.0",
  "description": "Sample DAICO built on top of DAOstack",
  "private": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile",
    "deploy-graph": "bash ops/hackyDeploy.sh",
    "ganache": "npm explore @daostack/migration -- npm run ganache",
    "migrate": "rm -rf ./node_modules/web3-providers-ws/node_modules/websocket && node migrations/DeployDao.js",
    "launch:docker": "docker-compose up -d",
    "stop": "docker-compose down"
  },
  "keywords": [
    "DAO",
    "DAOStack",
    "Ethereum",
    "Blockchain"
  ],
  "author": "Ben Kaufman",
  "license": "ISC",
  "dependencies": {
    "@daostack/arc": "0.0.1-rc.38",
    "@daostack/infra": "0.0.1-rc.15",
    "@daostack/migration": "0.0.1-rc.38-v2",
    "@daostack/subgraph": "0.0.36-10",
    "@daostack/client": "0.2.48",
    "dotenv": "^8.0.0",
    "node": "^10.16.3",
    "openzeppelin-solidity": "^2.1.3",
    "truffle": "5.1.9"
  },
  "devDependencies": {
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.1"
  }
}
