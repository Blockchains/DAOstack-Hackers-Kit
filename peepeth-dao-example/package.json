{
  "name": "peepdao",
  "version": "1.0.0",
  "description": "DAO for peeping on Peepeth",
  "private": true,
  "directories": {
    "test": "test"
  },
  "scripts": {
    "compile": "truffle compile",
    "ganache": "npm explore @daostack/migration -- npm run ganache",
    "ganache:docker": "docker-compose up -d",
    "migrate": "node migrations/deployDao.js",
    "stop": "docker-compose down",
    "webpack": "npx webpack --config webpack.config.js",
    "launch-local": "npm i && npm run ganache:docker && npm run migrate"
  },
  "keywords": [
    "DAO",
    "DAOStack",
    "Peepeth",
    "Ethereum",
    "Blockchain"
  ],
  "author": "Ben Kaufman",
  "license": "ISC",
  "dependencies": {
    "@daostack/arc": "0.0.1-rc.37",
    "@daostack/infra": "0.0.1-rc.15",
    "@daostack/migration": "0.0.1-rc.37-v3",
    "node": "^8.9.4",
    "openzeppelin-solidity": "^2.1.3",
    "truffle": "5.1.9",
    "dotenv": "^8.0.0",
    "ipfs-mini": "^1.1.2"
  },
  "devDependencies": {
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  }
}
